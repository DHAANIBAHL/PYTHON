import random
while True:
    user_choice=input("Roll the dice? (y/n): ")
    choice=user_choice.lower()
    if choice=='y':
        die1=random.randint(1,6)     
        die2=random.randint(1,6) 
        print(die1, die2)   
    elif choice=='n':
        print('Thanks for playing!')
        break
    else:
        print('Invalid input! Please try again.')

       
   

